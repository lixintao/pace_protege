    <!-- http://www.co-ode.org/ontologies/photography/photography.owl#hasDepthOfField -->

    <owl:DatatypeProperty rdf:about="#hasDepthOfField"/>
    


    <!-- http://www.co-ode.org/ontologies/photography/photography.owl#hasDuration -->

